{
  "name": "inventory-privacy",
  "private": true,
  "type": "module",
  "scripts": {
    "start:sui": "powershell -ExecutionPolicy Bypass -File scripts/start_sui.ps1",
    "fund": "powershell -ExecutionPolicy Bypass -File scripts/fund_address.ps1",
    "deploy": "powershell -ExecutionPolicy Bypass -File scripts/deploy_contracts.ps1",
    "server:start": "cargo run -p inventory-proof-server --release",
    "client:dev": "cd web && npm run dev",
    "bootstrap": "npm run fund && npm run deploy"
  },
  "dependencies": {
    "@mysten/sui": "^1.45.2"
  }
}
